xt\" v-model=\"user\" placeholder=\"Введите ник\">\n                <button type=\"submit\" @click=\"logIn\">Войти</button>\n            </form>\n            \n            <h1 class=\"main__header\">ЗАДАЧИ</h1>\n\n            <div class=\"main__table\">\n                <div class=\"main__table-collumn for_execution\" >\n                    <div class=\"main__table-data\"  v-for=\"(todo, index) in this.$store.getters.TODOS\" :key=\"index\">\n                        <todo-card v-if=\"todo.status==='К выполнению'\" :index=\"index\" :name=\"todo.name\" :description=\"todo.description\" :status=\"todo.status\" :img=\"todo.img\"/>\n                    </div>\n                </div>\n                <div class=\"main__table-collumn in_progress\" >\n                    <div class=\"main__table-data\" v-for=\"(todo, index) in this.$store.getters.TODOS\" :key=\"index\">\n                        <todo-card v-if=\"todo.status==='В процессе'\" :index=\"index\" :name=\"todo.name\" :description=\"todo.description\" :status=\"todo.status\" :img=\"todo.img\"/>\n                    </div>\n                </div>\n                <div class=\"main__table-collumn done\" >\n                    <div class=\"main__table-data\" v-for=\"(todo, index) in this.$store.getters.TODOS\" :key=\"index\">\n                        <todo-card v-if=\"todo.status==='Выполнено'\" :index=\"index\" :name=\"todo.name\" :description=\"todo.description\" :status=\"todo.status\" :img=\"todo.img\"/>\n                    </div>\n                </div>\n            </div>\n            <button type=\"button\" class=\"main__button\" @click=\"$router.push({name : 'add'})\">Добавить задачу</button>\n        </div>\n\n    </div>\n</template>\n\n<script>\nimport TodoCard from \"../components/TodoCard.vue\";\nexport default {\n    data()  {\n        return {\n            user : \"\",\n        }\n    },\n    components : {\n        TodoCard,\n    },\n    methods:{\n        logIn() {\n            this.$store.commit('SET_USER', this.user);\n            localStorage.setItem('user', this.user);\n            if(this.$store.getters.USER){\n                this.$emit('loggedIn');\n            }\n        },\n        logOut() {        \n            if(this.$store.getters.USER){\n                this.$emit('loggedOut');\n            }\n            localStorage.clear();\n            this.$store.commit('CLEAR');\n        },\n    },\n}\n</script>\n\n<style lang=\"scss\">\n    .main__table{\n        display: flex;\n        .main__table-collumn{\n            display: flex;\n            flex-direction: column;\n        }\n    }\n</style>"],"sourceRoot":""}]}